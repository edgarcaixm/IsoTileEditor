// Selection Detectionfunction getSelectionProps() {	if (fl.documents.length == 0) {		var nullArray = new Array();		return nullArray;	}	var dom = fl.getDocumentDOM();	if (dom.selection.length == 1 && dom.selection[0].instanceType == "symbol") {		var tItem = dom.selection[0].libraryItem;		if (tItem.getData("tileID") == 0) {			return;		}		var str = tItem.getData("tileID") + "," + tItem.getData("rows") + "," + tItem.getData("cols") + "," + tItem.getData("height") + "," + tItem.getData("isInteractive");		return str;	} else {		return;	}}function getDocID() {	if (fl.documents.length == 0) {		return "null";	}	return fl.getDocumentDOM().id;}