var dom = fl.getDocumentDOM();var library = dom.library;// Selection Detectionfunction getSelectionProps() {	if (dom.selection.length == 1 && dom.selection[0].instanceType == "symbol") {		var tItem = dom.selection[0].libraryItem;		if (tItem.getData("tileID") == 0) {			return;		}		var str = tItem.getData("tileID") + "," + tItem.getData("rows") + "," + tItem.getData("cols") + "," + tItem.getData("height");		return str;	} else {		return;	}}function getDocID() {	return fl.getDocumentDOM().id;}