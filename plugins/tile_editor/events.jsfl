function onDocChanged() {	var tDoc = fl.getDocumentDOM();	var args = tDoc.getDataFromDocument();	for (i=0; i<fl.swfPanels.length; i++) {		if (fl.swfPanels[i].name == "Diversion Tile Editor") {			fl.swfPanels[i].call("docChanged", args);			break;		}	}}// stores event ids in a local file so they can be cleaned up predictablyvar tmpFilePath = "file:///tmp/docClassCheckerIds.txt";if (FLfile.exists(tmpFilePath)) {	var contents = FLfile.read(tmpFilePath);	var lines = contents.split("\n");	var iLen = lines.length;	for (var i = 0; i < iLen; i++) {		var line = lines[i];		var pieces = line.split("=");		var eventType = pieces[0];		var eventId   = pieces[1];		fl.removeEventListener(eventType, parseInt(eventId));	}}// adds event listenervar changeId = fl.addEventListener("documentChanged", onDocChanged);FLfile.write(tmpFilePath, "documentChanged=" + changeId);